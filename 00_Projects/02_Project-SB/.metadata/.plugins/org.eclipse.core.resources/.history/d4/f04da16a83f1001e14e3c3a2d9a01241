package com.medimeet.main;

import java.time.LocalDate;
import java.util.Random;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;

@SpringBootApplication
public class MedimeetApplication {

	public static void main(String[] args) {
		SpringApplication.run(MedimeetApplication.class, args);
		
		
		LocalDate localDate = LocalDate.now();
		
		int dayOfMonth = localDate.getDayOfMonth();
		int monthValue = localDate.getMonthValue();
		int year = localDate.getYear();
		
		// generate random number between 1 - 10000
		Random random = new Random();

		int nextInt = random.nextInt(10000);

		System.out.println(nextInt);

		
		String dayOfMonthStr = ""+dayOfMonth+""+monthValue+""+year+""+random;
	}

}
